alias ls="eza"  
alias l="ls -1"
alias ll="ls -lh" 
alias la="ls -a"
alias lt="ls -lh -s time"
alias lla="ls -alh"
alias lg="ls -l --git"
alias cp="cp -v"
alias cd="z"
alias j="zi"
alias mv="mv -v"
alias vim="nvim"
alias grepp="grep * --color=auto -H -n -r -e "
alias grep="grep --color=auto"
alias starwars="telnet towel.blinkenlights.nl"
