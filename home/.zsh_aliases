# bash aliases
# alias ls="ls --color -Fh"
# alias ls="lsd --icon never"
alias ls="eza"
alias l="ls -1"
alias ll="ls -lh"
alias la="ls -a"
alias lt="ls -lh -s time"
alias lla="ls -alh"
alias lg="ls -l --git"
alias cp="cp -v"
alias cd="z"
alias j="zi"
alias mv="mv -v"
alias grepp="grep * --color=auto -H -n -r -e "
alias grep="grep --color=auto"
alias starwars="telnet towel.blinkenlights.nl"
alias brackets="nl -ba $* | egrep --color=always '[{}]'"
alias removeall="sed -i /$*/d **/*(.)"
alias calc="noglob calc"
alias ack="rg -L --hidden --stats --pretty -z"
alias cat="bat -p"
alias assume=". assume"
alias bandwhich="sudo bandwhich"
alias asitop="sudo asitop"
alias htop="sudo htop"
alias ssm="sshm"
alias ssr="sshm -s"
alias tailscale="/Applications/Tailscale.app/Contents/MacOS/Tailscale"
alias beachballing='~/.bin/hs -t 30 -c "hs.inspect(hs.window._timed_allWindows())" |sed "s/\[//g" | sed "s/\]//g" |sed "s/=/:/g"'
